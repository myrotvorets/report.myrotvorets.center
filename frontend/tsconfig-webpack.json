{
    "compilerOptions": {
        "target": "ES2019",
        "module": "CommonJS",
        "sourceMap": false,
        "strict": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "noFallthroughCasesInSwitch": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "skipDefaultLibCheck": true
    },
    "include": ["webpack.*.ts", ".webpack/*.ts"],
    "compileOnSave": true
}
