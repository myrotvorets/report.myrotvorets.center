{
    "compilerOptions": {
        "target": "es2019",
        "module": "commonjs",
        "allowJs": false,
        "jsx": "react",
        "jsxFactory": "h",
        "sourceMap": true,
        "strict": true,
        "moduleResolution": "node",
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "noFallthroughCasesInSwitch": true,
        "esModuleInterop": true,
        "skipDefaultLibCheck": true
    },
    "include": ["webpack.*.ts", ".webpack/*.ts", "setupTests.ts", "src/**/*.ts", "src/**/*.tsx"]
}
